{
  "version": 3,
  "name": "quntum-api",
  "timeout": "5000ms",
  "cache_ttl": "300s",
  "output_encoding": "no-op",
  "port": 3890,
  "plugin": {
    "pattern": ".so",
    "folder": "/etc/krakend/plugins/"
  },
  "endpoints": [
    {
      "endpoint": "/quntum-api/api",
      "method": "GET",
      "output_encoding": "no-op",
      "extra_config": {
        "security/cors": {
          "allow_origins": [
            "*"
          ],
          "allow_methods": [
            "GET"
          ],
          "expose_headers": [
            "*"
          ],
          "allow_headers": [
            "*"
          ],
          "max_age": "12h",
          "allow_credentials": false
        }
      },
      "backend": [
        {
          "url_pattern": "api",
          "encoding": "json",
          "sd": "static",
          "method": "GET",
          "host": [
            "https://quantum-admin-api-dev.konneqt.cloud/"
          ],
          "disable_host_sanitize": false,
          "extra_config": {
            "plugin/http-client": {
              "name": "qap-auth-injector"
            }
          }
        }
      ]
    }
  ]
}