{
  "version": 3,
  "name": "tvcs",
  "timeout": "5000ms",
  "cache_ttl": "300s",
  "output_encoding": "no-op",
  "port": 3890,
  "endpoints": [
    {
      "endpoint": "/tvcs//travelexplore-api/destinations",
      "method": "GET",
      "output_encoding": "no-op",
      "extra_config": {
        "security/cors": {
          "allow_origins": [
            "*"
          ],
          "allow_methods": [
            "GET",
            "POST",
            "HEAD"
          ],
          "expose_headers": [
            "Content-Length",
            "Content-Type"
          ],
          "allow_headers": [
            "Accept-Language"
          ],
          "max_age": "12h",
          "allow_credentials": false
        }
      },
      "backend": [
        {
          "url_pattern": "/v1/destinations",
          "encoding": "no-op",
          "sd": "static",
          "method": "GET",
          "host": [
            "https://api.travelexplore.com"
          ],
          "disable_host_sanitize": false
        }
      ]
    },
    {
      "endpoint": "/tvcs//css/cores",
      "method": "GET",
      "output_encoding": "no-op",
      "extra_config": {
        "security/cors": {
          "allow_origins": [
            "*"
          ],
          "allow_methods": [
            "GET"
          ],
          "expose_headers": [
            "*"
          ],
          "allow_headers": [
            "*"
          ],
          "max_age": "12h",
          "allow_credentials": false
        }
      },
      "backend": [
        {
          "url_pattern": "/csscolornames/colors",
          "encoding": "json",
          "sd": "static",
          "method": "GET",
          "host": [
            "https://api.sampleapis.com"
          ],
          "disable_host_sanitize": false
        }
      ]
    }
  ]
}