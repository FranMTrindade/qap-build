"{\n  \"info\": {\n    \"title\": \"MediTravel\",\n    \"version\": \"0.1.9\",\n    \"description\": \"The MediTravel API offers an integrated solution that combines travel resources and medical services in a single interface. This API allows developers to access and manage information related to medical travel,\"\n  },\n  \"tags\": [],\n  \"paths\": {\n    \"/meditravel/medical-api/doctors\": {\n      \"get\": {\n        \"summary\": \"Retrieve Doctors\",\n        \"responses\": {\n          \"200\": {\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"type\": \"object\"\n                }\n              }\n            },\n            \"description\": \"Successful Response\"\n          }\n        },\n        \"parameters\": []\n      }\n    },\n    \"/meditravel/medical-api/specialties\": {\n      \"get\": {\n        \"summary\": \"Retrieve Specialties\",\n        \"responses\": {\n          \"200\": {\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"type\": \"object\"\n                }\n              }\n            },\n            \"description\": \"Successful Response\"\n          }\n        },\n        \"parameters\": []\n      }\n    },\n    \"/meditravel/travelexplore-api/destinations\": {\n      \"get\": {\n        \"summary\": \"Get a list of travel destinations\",\n        \"responses\": {\n          \"200\": {\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"type\": \"array\",\n                  \"items\": {\n                    \"$ref\": \"#/components/schemas/Destination\"\n                  }\n                }\n              }\n            },\n            \"description\": \"List of destinations returned successfully\"\n          }\n        },\n        \"parameters\": [\n          {\n            \"in\": \"query\",\n            \"name\": \"region\",\n            \"schema\": {\n              \"type\": \"string\",\n              \"example\": \"Europe\"\n            },\n            \"required\": false\n          },\n          {\n            \"in\": \"query\",\n            \"name\": \"country\",\n            \"schema\": {\n              \"type\": \"string\",\n              \"example\": \"France\"\n            },\n            \"required\": false\n          },\n          {\n            \"in\": \"query\",\n            \"name\": \"popularity\",\n            \"schema\": {\n              \"type\": \"string\",\n              \"example\": \"Top Rated\"\n            },\n            \"required\": false\n          }\n        ],\n        \"description\": \"Retrieve a list of travel destinations based on various filters such as region, country, or popularity.\"\n      }\n    },\n    \"/meditravel/travelexplore-api/accommodations\": {\n      \"get\": {\n        \"summary\": \"Get accommodation options\",\n        \"responses\": {\n          \"200\": {\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"type\": \"array\",\n                  \"items\": {\n                    \"$ref\": \"#/components/schemas/Accommodation\"\n                  }\n                }\n              }\n            },\n            \"description\": \"List of accommodations returned successfully\"\n          }\n        },\n        \"parameters\": [\n          {\n            \"in\": \"query\",\n            \"name\": \"destinationId\",\n            \"schema\": {\n              \"type\": \"integer\"\n            },\n            \"required\": true\n          }\n        ],\n        \"description\": \"Retrieve accommodation options such as hotels, vacation rentals, and hostels for a specified destination.\"\n      }\n    },\n    \"/meditravel/travelexplore-api/destinations/{destinationId}\": {\n      \"get\": {\n        \"summary\": \"Get destination details\",\n        \"responses\": {\n          \"200\": {\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Destination\"\n                }\n              }\n            },\n            \"description\": \"Destination details returned successfully\"\n          }\n        },\n        \"parameters\": [\n          {\n            \"in\": \"path\",\n            \"name\": \"destinationId\",\n            \"schema\": {\n              \"type\": \"integer\"\n            },\n            \"required\": true\n          }\n        ],\n        \"description\": \"Retrieve detailed information about a specific destination, including tourist attractions, accommodations, and travel tips.\"\n      }\n    }\n  },\n  \"openapi\": \"3.0.0\",\n  \"servers\": [\n    {\n      \"url\": \"/\",\n      \"description\": \"Main (production) server\"\n    },\n    {\n      \"url\": \"/\",\n      \"description\": \"Internal test server\"\n    }\n  ],\n  \"security\": [\n    {\n      \"bearerAuth\": []\n    }\n  ],\n  \"components\": {\n    \"schemas\": {},\n    \"requestBodies\": {},\n    \"securitySchemes\": {\n      \"bearerAuth\": {\n        \"type\": \"http\",\n        \"scheme\": \"bearer\",\n        \"bearerFormat\": \"JWT\"\n      }\n    }\n  }\n}"